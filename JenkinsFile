node () {

env.

deleteDir()

      stage ('Checkout Build Code') {
         checkout scm
       }

      stage ('Get vRA Token Image'){
        sh 'get_token.sh'
      }

      stage ('Terrraform Init')
      {
        sh '/usr/bin/terraform.12.26 init'
      }

      stage ('Terrraform Plan')
      {
        sh '/usr/bin/terraform.12.26 plan -var refresh_token="$VRA_REFRESH_TOKEN" -out myplan '
      }
}